TestTypes
{
    theory
    {
        pred Equal(a,b: tpl)
        {
            p: pred

			all p
			(
				iif
				(
					p(a),
					p(b)
				)
			)
        }


        func BinOp(x,y: tplSetElem) -> tplSetElem
        {
            optional pred IsLeftNeutralElement(e: tplSetElem)
            {
                Equal(@self(e,x), x)
            }

            optional tplSetElem LeftNeutralElement()
            {
                assert IsLeftNeutralElement(self)
            }
        }
    }
}

