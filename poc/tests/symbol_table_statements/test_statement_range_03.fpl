TestTheory
{
    theory
    {
        func RiemannIntegral(from,to:Real, f: RealValuedFunction ) -> Real
        {
            x, result: Real
            assert IsBounded(f)
            result:=ZeroReal()
            range x [a~b]
            (
                result:=Add(result, Mult(f(x), D(x)))
            )
            return result
        }

    }

}


