TestTheory
{
    theory
    {
        class ZeroVectorN: Tuple
        {
            ZeroVectorN(n: Nat, field: Field)
            {
                from, to: Nat
                from := Nat(1)
                zero: tplFieldElem[from~to]
                i: Nat
                range i [1~n]
                (
                    self[i]:=field.AdditiveGroup().NeutralElement()
                )
            }
        }


    }

}


