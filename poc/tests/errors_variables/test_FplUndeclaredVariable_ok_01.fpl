TestNamespace
{
    theory
    {
        prop TestProposition(op: Add)
        {
            n,m: Nat
            anotherAdd: Add
            pre: undef
            con: undef
        }

        proof TestProposition$1
        {
            1. assume Equal(n,m)
        }
    }
}

